{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard-with-typescript",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier",
    "react-hooks",
    "@typescript-eslint",
    "import"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error", {}, { "endOfLine": "auto" }],
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
